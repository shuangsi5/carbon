{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "*.html",
        "use": "@vercel/static"
      },
      {
        "src": "*.css",
        "use": "@vercel/static"
      },
      {
        "src": "*.js",
        "use": "@vercel/static"
      },
      {
        "src": "*.png",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/login",
        "dest": "/login.html"
      },
      {
        "src": "/register",
        "dest": "/register.html"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
